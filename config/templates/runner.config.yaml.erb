name: <%= node['hostname'] %>
labels: ["<%= node['runner']['labels'] %>"]

log:
  level: info

runner:
  file: .runner
  capacity: 1
  timeout: 1h
  labels: ["<%= node['runner']['labels'] %>"]

cache:
  enabled: true
  dir: "<%= node['runner']['cache_dir'] %>"

host:
  workdir_parent: "<%= node['runner']['cache_dir'] %>"
